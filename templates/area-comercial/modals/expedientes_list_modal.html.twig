<div class="modal fade" id="expediente_list_modal" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">{{ "Expedientes_list"|trans }}</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="table-responsive">
                    <table class="table table-sm table-hover fs-12" id="expediente_list">
                        <thead class="thead-light">
                        <tr>
                            <th>{{ 'Pipeline_name'|trans }}</th>
                            <th>{{ "Office"|trans }}</th>
                            <th>{{ "Amount"|trans }}</th>
                            <th>{{ 'Client'|trans }}</th>
                            <th>{{ "State"|trans }}</th>
                            <th>{{ "CreatedAt"|trans }}</th>
                            <th>{{ "Description"|trans }}</th>
                        </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="modal-footer">
                <input type="hidden" id="isCopy">
                <button id="button_copiar_datos_exp" type="button" class="btn btn-blue btn-sm"><i class="fa fa-save"></i> {{ "button_copiar_datos_exp"|trans }}</button>
                <button type="button" class="btn btn-sm" data-dismiss="modal"><i class="fa fa-minus-circle"></i> {{ "Close"|trans }}</button>
            </div>
        </div>
    </div>
</div>
